{
  "name": "vocalcoach-ai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "audit:performance": "node scripts/performance-audit.js",
    "audit:ci": "cross-env CI=true npm run audit:performance",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:bundle:ci": "cross-env CI=true npm run analyze:bundle",
    "audit:accessibility": "node scripts/accessibility-audit.js",
    "audit:accessibility:ci": "cross-env CI=true npm run audit:accessibility",
    "test:load": "node scripts/load-test.js",
    "test:load:ci": "cross-env CI=true npm run test:load",
    "beta:setup": "node scripts/setup-beta.js",
    "beta:setup:ci": "cross-env CI=true npm run beta:setup",
    "beta:send-invites": "node scripts/send-beta-invites.js",
    "beta:send-invites:ci": "cross-env CI=true npm run beta:send-invites",
    "beta:monitor": "node scripts/monitor-health.js",
    "beta:monitor:ci": "NODE_ENV=ci node scripts/monitor-health.js",
    "beta:monitor:background": "node scripts/monitor-health.js &",
    "beta:report": "node scripts/generate-beta-report.js",
    "beta:report:ci": "cross-env CI=true npm run beta:report",
    "beta:report:daily": "node scripts/generate-beta-report.js --daily",
    "beta:report:weekly": "node scripts/generate-beta-report.js --weekly",
    "beta:schedule-reports": "node scripts/schedule-beta-reports.js",
    "beta:schedule-reports:ci": "cross-env CI=true npm run beta:schedule-reports",
    "beta:backup": "node scripts/backup-beta.js",
    "beta:backup:ci": "cross-env CI=true npm run beta:backup",
    "beta:backup:daily": "node scripts/backup-beta.js --daily",
    "beta:verify-db": "node scripts/verify-mongodb.js",
    "beta:verify-db:ci": "cross-env CI=true npm run beta:verify-db",
    "beta:validate-email": "node scripts/validate-email.js",
    "beta:validate-email:ci": "NODE_ENV=ci node scripts/validate-email.js",
    "beta:setup-gmail": "node scripts/setup-gmail.js",
    "beta:setup-gmail:ci": "NODE_ENV=ci node scripts/setup-gmail.js",
    "beta:backup-db": "node scripts/backup-mongodb.js",
    "beta:backup-db:ci": "NODE_ENV=ci node scripts/backup-mongodb.js",
    "beta:backup-db:schedule": "node -e \"setInterval(() => require('./scripts/backup-mongodb.js'), 24 * 60 * 60 * 1000)\"",
    "beta:setup-dns": "node scripts/setup-email-dns.js generate",
    "beta:verify-dns": "node scripts/setup-email-dns.js verify",
    "beta:setup-dns:ci": "NODE_ENV=ci npm run beta:setup-dns",
    "beta:test-restore": "node scripts/test-restore.js --source vocalcoach_staging --target vocalcoach_staging_test",
    "beta:test-discord": "node scripts/discord-alerts.js",
    "beta:test-discord:ci": "NODE_ENV=ci npm run beta:test-discord",
    "beta:setup-staging": "node scripts/setup-staging.js",
    "beta:backup-staging": "node scripts/backup-mongodb.js --db vocalcoach_staging"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.7",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@react-spring/web": "^9.7.3",
    "@sentry/react": "^7.64.0",
    "@sentry/tracing": "^7.64.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.31",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "discord.js": "^14.14.1",
    "mixpanel-browser": "^2.47.0",
    "mongodb": "^6.3.0",
    "node-cron": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.7.3",
    "@types/mixpanel-browser": "^2.47.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "autocannon": "^7.12.0",
    "chrome-launcher": "^0.15.2",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "lighthouse": "^9.6.8",
    "nodemailer": "^7.0.3",
    "puppeteer": "^21.3.8",
    "terser-webpack-plugin": "^5.3.9",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
